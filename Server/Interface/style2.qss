QStatusBar
{ 
	/*color : #121212 ;*/
	
	border-top : 1px solid #121212;
	
	font : 20px;

}


/*################################################### BUTTONS #########################################################*/
/* Style of buttons in the sidebar  START  */

QPushButton#sidebar_button{
	background : #ffffff;
	color: #121212;
	text-align : left; 
	padding-left : 20; 	
	border-radius : 0px;
	margin-top: 2px;
	
}
QPushButton#sidebar_button::focus{
	background : #aaaaaa;
	color: #121212;
}

/* Style of buttons in the sidebar  END  */


/* Style of buttons in the topbar  START  */

QPushButton#topbar_button{
	font-size : 18px;
	text-align : center; 	
	border : 1px solid #ADFF2F;
	border-radius : 25px;
	background : #ffffff;
	color : #121212;
	
}

/* Style of buttons in the topbar  END  */




/* Style of interior buttons  START  */

QPushButton#interior_button{
	border-top-right-radius: 10px;
	border-bottom-right-radius: 0px; 
	font-size : 14px;
	text-align : center; 	
	border : 1px solid #ADFF2F;
	background : #ffffff;
}

QPushButton#interior_button::focus
{
	color : #060606;

}

QPushButton#interior_button::hover::focus
{
	color : #060606;

}

QPushButton#interior_button::pressed
{
	color : #000000;
	
}

QPushButton#interior_button::hover
{
	color : #121212;
	
}

QPushButton#interior_button::disabled{
	color: #E91E63;
}
QPushButton#interior_button::!enabled{
	color: #E91E63;
}


/* Style of interior buttons END  */





/* Style of buttons in the close dialog box  START  */
QPushButton#close_button_yes{
	text-align : center;
	background : #ffffff;
	color : #990000;
}

QPushButton#close_button_yes::focus{
	color: #AA0000;
}

QPushButton#close_button_yes::focus::hover{
	color: #FF0000;
}

QPushButton#close_button_yes::hover{
	color: #FF0000;
}

QPushButton#close_button_no{
	text-align : center;
	background : #ffffff;
	color : #009900;
}

QPushButton#close_button_no::focus{
	color: #00AA00;
}

QPushButton#close_button_no::focus::hover{
	color: #00FF00;
}

QPushButton#close_button_no::hover{
	color: #FF0000;
}
/* Style of buttons in the close dialog box  END  */


QPushButton#account_button
{
	text-align : center;
	margin-left : 70px;
	border : 1px solid #ADFF2F;
	border-radius : 25px;
}

QRadioButton{
	color : #999999;
}
QRadioButton#interior_rbutton::indicator{
	width :16px;
	height: 16px;
	border-radius : 8px;


}
QRadioButton#interior_rbutton::indicator::unchecked{
	image: url(./Elements/radio_unchecked.png);
}

QRadioButton#interior_rbutton::indicator::checked{
	image: url(./Elements/radio_checked.png);
}
/*################################################### /BUTTONS #########################################################*/


