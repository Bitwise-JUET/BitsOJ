/*--------------MAIN LAYOUT STARTS----------------*/

/* Sidebar style */
QWidget#sidebar{
	margin-left : 0px;
}

/* Right panes style START */

/* right panes background color */
QTabWidget#main_tabs{
	background-color : #ffffff;
}

/* Text color in right panes */
QTabWidget::pane{
	color : #121212;
}
/* Right panes style END */


/* Style of background */
QWidget#screen_widget
{
	color : #121212;
	background-color : #ffffff;
}

/* Style of bar where bitwise logo sits */
QWidget#main_widget{
	background-color: #ffffff;
	color: #121212;
}

/* Style of content boxes*/
QWidget#content_box{
	border: 1px solid #888888;
}


/* Style of content */

QLabel#main_screen_content{
	color : #121212;
	font-size : 20px;
}

QLabel#main_screen_heading{
	color : #111111;
	font-size : 30px;
}

QLabel#main_screen_sub_heading{
	color : #444444;
	font-size : 26px;
}

QLabel#about_screen_heading{
	margin-top : 200px;
	color : #121212;
	font-size : 30px;
}
QLCDNumber{
	color: #121212;
	border : 0px;
}

/* Style of tabs in tab-bar (basically invisible)  START  */
QTabBar::tab
{
	width: 0; 
	height: 0; 
	margin: 0; 
	padding-top: 10; 
}
/* Style of tabs in tab-bar (basically invisible)  END  */

/*Style of problem tab bar*/
QTabBar#problem_tabs
{
	background : #ffffff;
}

QTabBar#problem_tabs::tab
{
	width: 140px; 
	height: 35px; 
	margin: 0px; 
	background : #ffffff;
	color : #777777;
}

QTabBar#problem_tabs::tab:selected
{ 
	color : #E91E63;
	border-top : 1px solid #B2FF59;
}



/*--------------MAIN LAYOUT ENDS----------------*/





/*--------------SUB LAYOUT STARTS---------*/
QDialog{
	background-color : #ffffff;
}
QDialog QLabel{
	color : #121212;
}
QDialog QPushButton{
	text-align : center;
	background-color : #ffffff ; 
	color : #121212;
	border : 1px solid #121212;
}

QSpinBox{
	background : #ffffff;
	color : #121212;
	border : 1px solid #777777;
	height : 30px;
}
QSpinBox::up-button 
{
    subcontrol-origin: border;
    subcontrol-position: top right; 
    width: 16px; 
    border : 1px solid #777777;
    border-width: 1px;
}
QSpinBox::up-arrow 
{
    image: url(./Elements/up_arrow.png);
    width: 10px;
    height: 10px;
}
QSpinBox::down-button 
{
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 16px; 
    border : 1px solid #777777;
    border-width: 1px;
}
QSpinBox::down-arrow 
{
    image: url(./Elements/down_arrow.png);
    width: 10px;
    height: 10px;
}

QComboBox#account_combobox
{
	background : #ffffff;
	color : #121212;
	selection-background : #777777;
	border: 1px solid #777777;
    border-radius: 3px;
    height : 30px;
}
QComboBox#account_combobox::drop-down 
{
	background : #ffffff;
    subcontrol-origin: padding;
    subcontrol-position: top right;
    border-left-width: 1px;
    border-left-color: darkgray;
    border-left-style: solid; 
    border-top-right-radius: 3px;
    border-bottom-right-radius: 3px;
}
QComboBox QAbstractItemView 
{		/* This is the style of dropdown menu*/
    background-color : #ffffff;
	color : #121212;
	border : 1px solid #2D353D;
	selection-background-color : #777777;
}
QComboBox::down-arrow 
{
    image: url(./Elements/down_arrow.png);
    width: 10px;
    height: 10px;
}

QLabel#account_label
{
	color : #121212;
}

QWidget#account_window
{
	background-color : #ffffff;
}
QTextEdit
{
	background : #ffffff;
	color : #121212;
}

QLineEdit
{
	height : 30px;
	background : #ffffff;
	color : #121212;
	border : 1px solid #777777;
}




/*--------------SUB LAYOUT ENDS---------*/

/*################################################ /LAYOUT OBJECTS ######################################################*/



/*################################################ TABLE OBJECTS ######################################################*/

/* Table styles */
QTableView
{
	background : #ffffff;
	alternate-background-color : #eeeeee;
	color : #121212;
	border : 0px;
	gridline-color : #transparent;
	selection-background-color : #777777;
}

QTableView::item:selected
{
	border : 2px solid #0891D4;
}

QHeaderView::section
{ 
	background-color : #999999;
	color: #121212;	
}

QTableWidget#inner_table
{
	background : #ffffff;
	border : 1px solid #555555;
}

/* Scroll bar styles */
QScrollBar:horizontal
{
	height : 0px;
}

QScrollBar:vertical{
	margin-left : 1px;
	width : 25px;
	background-color : #777777;	
	border: 1px solid #121212;
}

QScrollBar::handle:vertical{
	background : #aaaaaa;
	width : 25px;
	min-height : 500px;
	margin-left : 0px;
	margin-top : 1px;
	margin-bottom : 1px;
	margin-right : 0px;
}

QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
	width : 0px;
	height : 0px;
}

/*################################################ /TABLE OBJECTS ######################################################*/

